{
  "name": "arxiv-downloader",
  "version": "1.0.0",
  "description": "A command-line tool for downloading and managing arXiv papers, including both PDF and LaTeX source files.",
  "main": "dist/index.js",
  "bin": {
    "adown": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "npm run build"
  },
  "keywords": [
    "arxiv",
    "cli",
    "downloader",
    "papers",
    "research",
    "pdf",
    "latex"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "commander": "^9.5.0",
    "fs-extra": "^11.1.1",
    "inquirer": "^8.2.6",
    "mkdirp": "^3.0.1",
    "os": "^0.1.2",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^8.2.6",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  }
}
